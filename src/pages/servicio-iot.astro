---
import type { GetStaticPaths } from "astro";
import Layout from "@src/layouts/Layout.astro";
import Hero from "@src/components/ServiciosIoT/Hero.astro";
import HighLights from "@src/components/ServiciosIoT/HighLights.astro";
import Ubication from "@src/components/Home/Ubication.astro";
import ExitCases from "@src/components/Servicios/ExitCases.astro";
import OurProcess from "@src/components/ServiciosIoT/OurProcess.astro";
import Techs from "@src/components/ServiciosIoT/Techs.astro";
import Values from "@src/components/ServiciosIoT/Values.astro";

export const getStaticPaths = (() => {
  // Las uri se usan para obtener el objeto del archivo Specialites.ts
  return [{ params: { service: "servicio-hardware" } }];
}) satisfies GetStaticPaths;
---

<Layout title="Servicios MMJ">
  <Hero />
  <HighLights />
  <Techs />
  <Values />
  <ExitCases />
  <OurProcess />
  <Ubication />
</Layout>
