---
import type { GetStaticPaths } from "astro";
import Hero from "@src/components/Carreras/Hero.astro";
import Courses from "@src/components/Carreras/Courses.astro";
import Layout from "@src/layouts/Layout.astro";
import Plans from "@src/components/Carreras/Plans.astro";
import Ubication from "@src/components/Home/Ubication.astro";

export const getStaticPaths = (() => {
  // Las uri se usan para obtener el objeto del archivo Specialites.ts
  return [
    { params: { carrera: "embebido" } },
    { params: { carrera: "hardware" } },
    { params: { carrera: "software" } },
    { params: { carrera: "iot" } },
    { params: { carrera: "presencial" } },
  ];
}) satisfies GetStaticPaths;
---

<Layout title="Carreras MMJ">
  <Hero />
  <Courses />
  <!-- <Projects /> -->
  <!-- <NextLaunch /> -->
  <Plans />
  <Ubication />
</Layout>
